---
layout: null
permalink: /changes/starrydata2/feed.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title>{{ site.title }} - starrydata2 (Changes)</title>
    <link>{{ site.url }}/changes/starrydata2/</link>
    <description>{{ site.description }}</description>
    {% assign filtered_changes = site.changes | where_exp:"item", "item.url contains '/changes/starrydata2/'" %}
    {% for item in filtered_changes %}
    <item>
      <version>{{ item.version }}</version>
      <title>{{ item.title }}</title>
      <link>{{ site.url }}{{ item.url }}</link>
      <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
      <description>{{ item.description }}</description>
    </item>
    {% endfor %}
  </channel>
</rss>
